<td>query</td>
<td>string</td>
<td>false</td>
<td>none</td>
</tr>
<tr>
<td>inScopeOnly</td>
<td>query</td>
<td>string</td>
<td>false</td>
<td>none</td>
</tr>
<tr>
<td>scanPolicyName</td>
<td>query</td>
<td>string</td>
<td>false</td>
<td>none</td>
</tr>
<tr>
<td>method</td>
<td>query</td>
<td>string</td>
<td>false</td>
<td>none</td>
</tr>
