<span class="n">r</span> <span class="o">=</span> <span class="n">requests</span><span class="p">.</span><span class="n">get</span><span class="p">(</span><span class="s">'http://zap/JSON/spider/action/scanAsUser/'</span><span class="p">,</span> <span class="n">params</span><span class="o">=</span><span class="p">{</span>
