  <span class="s">'string'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'location'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'match'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'inverse'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'ignorecase'</span><span class="p">:</span> <span class="s">'string'</span>
