<span class="n">r</span> <span class="o">=</span> <span class="n">requests</span><span class="o">.</span><span class="n">get</span><span class="p">(</span><span class="s">'http://zap/JSON/httpSessions/action/setActiveSession/'</span><span class="p">,</span> <span class="n">params</span><span class="o">=</span><span class="p">{</span>
  <span class="s">'site'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'session'</span><span class="p">:</span> <span class="s">'string'</span>
<span class="p">},</span> <span class="n">headers</span> <span class="o">=</span> <span class="n">headers</span><span class="p">)</span>

