  <span class="s">'browser'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'argument'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'enabled'</span><span class="p">:</span> <span class="s">'string'</span>
