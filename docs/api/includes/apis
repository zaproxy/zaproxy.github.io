  <span class="s1">'Accept'</span> <span class="o">=&gt;</span> <span class="s1">'application/json'</span><span class="p">,</span>
  <span class="s1">'X-ZAP-API-Key'</span> <span class="o">=&gt;</span> <span class="s1">'API_KEY'</span>
<span class="p">}</span>

