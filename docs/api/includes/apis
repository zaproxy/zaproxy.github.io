  <span class="s">'channelId'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'outgoing'</span><span class="p">:</span> <span class="s">'string'</span><span class="p">,</span>  <span class="s">'message'</span><span class="p">:</span> <span class="s">'string'</span>
