  <span class="ss">params: </span><span class="p">{</span>
  <span class="s1">'contextId'</span> <span class="o">=&gt;</span> <span class="s1">'integer'</span><span class="p">,</span>
<span class="s1">'userId'</span> <span class="o">=&gt;</span> <span class="s1">'integer'</span><span class="p">,</span>
<span class="s1">'name'</span> <span class="o">=&gt;</span> <span class="s1">'string'</span>
<span class="p">},</span> <span class="ss">headers: </span><span class="n">headers</span>

<span class="nb">p</span> <span class="no">JSON</span><span class="p">.</span><span class="nf">parse</span><span class="p">(</span><span class="n">result</span><span class="p">)</span>

</code></pre>
<p><code>GET /JSON/users/action/setUserName/</code></p>

